    "sequelize": "^6.32.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.5.9",
    "@types/sequelize": "^4.28.15",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "eslint": "^8.23.1",
    "jest": "^29.0.3",
    "nodemon": "^3.0.1",
    "sequelize-cli": "^6.6.1",
